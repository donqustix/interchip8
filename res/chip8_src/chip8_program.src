jp start

sprite: byte 0, 1, 2, 4, 5

start:
    ld v0, 0x6 ;ddaw
    ld F, v0   

    ld v0, 16
    ld v1, 8
    drw v0, v1, 5          

    ld v0, 21
    ld v1, 8
    drw v0, v1, 5

    ld v0, 26
    ld v1, 8
    drw v0, v1, 5

    ld DT, v0

    ld v0, 10
    ld DT, v0

wait:
    ld v0, DT
    sne v0, 0
    jp start
    jp wait

